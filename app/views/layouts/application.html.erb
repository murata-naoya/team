<!DOCTYPE html>
<html>
  <head>
    <title>Team</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <ul>
      <% if admin_signed_in? %>
        <li><%= link_to "受注一覧", "#" %></li>
        <li><%= link_to "商品登録", "#" %></li>
        <li><%= link_to "ログアウト", destroy_admin_session_path %></li>
      <% elsif enduser_signed_in? %>
        <li><%= link_to "マイページ", "#" %></li>
        <li><%= link_to "カート一覧", "#" %></li>
        <li><%= link_to "ログアウト", destroy_enduser_session_path %></li>
      <% else %>
        <li><%= link_to "新規登録", new_enduser_registration_path %></li>
        <li><%= link_to "ログイン", new_enduser_session_path %></li>
      <% end %>
    </ul>
    <%= yield %>
  </body>
</html>
